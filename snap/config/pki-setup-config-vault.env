CREATE_CA=true
#
SETUP_HOME=.
SETUP_PKI=${SETUP_HOME}/pki
#
# TLS Server FQDN: "HOST.DOMAIN"
#
HOST=localhost
DOMAIN=local
#
# TLS server and CA organisation
#
ORG="EdgeXFoundry"
#
# TLS Server Certificate
#
TLS_C="US"
TLS_ST="CA"
TLS_L="San Francisco"
TLS_O=${ORG}
#
# CA Cerificate
#
CA_C="US"
CA_ST="CA"
CA_L="San Francisco"
CA_O=${ORG}
#
# PKCS12 container password
PKCS12_PASSWORD="REPLACE_ME_IN_INSTALL_HOOK"
#
